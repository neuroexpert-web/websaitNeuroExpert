{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/frontend/build/static/$1"
      },
      {
        "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|json))",
        "dest": "/frontend/build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/build/index.html"
      }
    ]
  }
  